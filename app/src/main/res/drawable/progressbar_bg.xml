<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#ff9a66"></solid>
            <stroke
                android:color="#ff9a66"
                android:width="0dp"
                android:dashWidth="0dp"
                android:dashGap="0dp" />
            <padding
                android:left="0dp"
                android:right="0dp"
                android:top="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#01b0f1"></solid>
            </shape>
        </clip>
    </item>


</layer-list>